@model SalesViewModel

<table class="table table-bordered verticle-middle table-responsive-lg">
    <thead>
        <tr>
            <th >S/No</th>
            <th >Food or Drink</th>
            <th >Price</th>
            <th >Quantity</th>
            <th >Total</th>
            <th>
                <a class="btn btn-primary text-white" data-toggle="modal" data-target="#sale_dialog">Add Items</a>
                @await Html.PartialAsync("_ItemSalePopup",null)
            </th>
        </tr>
    </thead>
    <tbody></tbody>
</table>


